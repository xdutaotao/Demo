.noticeClass{
  width: 100%;
  height: 160px;
}


.userinfo {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.userinfo-avatar {
  width: 128px;
  height: 128px;
  margin: 20rpx;
  border-radius: 50%;
}

.userinfo-nickname {
  margin: 10px
}

.listClass{
  display: flex;
  flex-direction: row;
  width: 100%;
  height: 100px;
  align-items: center;
}

.blockClass{
  display: flex;
  flex-direction: column;
}

.divider{
  height: 2px;
  background-color: #f3f3f3
}

.headClass{
  width: 100px;
  height: 80px;
  margin: 10px;
}

.stepCntClass{
  color: #333333
}

.rankClass{
  color: #15abc4
}

.noticeDetail{
  display: flex;
  flex-direction: column;
  justify-content: space-between
}

.title{
  color: #15abc4;
  font-size: 18px;
}

.time{
  color: #999999;
  font-size: 12px;
}
